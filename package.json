{
  "name": "reserve-me-server",
  "version": "0.0.1",
  "description": "",
  "author": "Jakub Naumowicz",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "start": "npm run build && NODE_ENV=production node dist/main.js",
    "deploy": "rm -rf node_modules package-lock.json && npm install --omit=dev && npm run start",
    "start:local": "NODE_ENV=local nest start --debug --watch",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "ts-patch install -s && nest build -p tsconfig.test.json && cp ./test/jest-e2e.json ./dist-test/test/",
    "test:e2e": "NODE_ENV=local jest --config ./dist-test/test/jest-e2e.json",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate deploy",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate deploy"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.693.0",
    "@nestjs-cognito/auth": "^1.1.7",
    "@nestjs-cognito/graphql": "^2.1.7",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/platform-express": "^10.4.8",
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "nestjs-prisma": "^0.23.0",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.0",
    "@nestjs-cognito/testing": "^1.1.5",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.6",
    "@types/supertest": "^6.0.2",
    "check-peer-dependencies": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vegardit/prisma-generator-nestjs-dto": "^1.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "jest-ts-auto-mock": "^2.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.22.0",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-docs-generator": "^0.8.0",
    "prisma-generator-fake-data": "^0.14.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-auto-mock": "^3.7.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "(.spec|.e2e-spec).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "setupFiles": [
      "<rootDir>auto-mock-config.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
