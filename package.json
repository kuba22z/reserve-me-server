{
  "name": "reserve-me-server",
  "version": "0.0.1",
  "description": "",
  "author": "Jakub Naumowicz",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node src/main2.js",
    "start:local": "NODE_ENV=local nest start --debug --watch",
    "start:dev": "NODE_ENV=dev nest start",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate deploy",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate deploy",
    "deploy": "npm ci && npm run build && npm run start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "ts-patch install -s",
    "pretest:e2e": "nest build -p tsconfig.test.json && cp ./test/jest-e2e.json ./dist-test/test/",
    "test:e2e": "NODE_ENV=local jest --config ./dist-test/test/jest-e2e.json"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "engineStrict": true,
  "check-node-version": "node check-node-version.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@aws-sdk/client-cognito-identity-provider": "^3.556.0",
    "@aws-sdk/credential-providers": "^3.556.0",
    "@nestjs-cognito/auth": "^1.1.6",
    "@nestjs-cognito/core": "^1.3.0",
    "@nestjs-cognito/graphql": "^2.1.6",
    "@nestjs-cognito/testing": "^1.1.3",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.1",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/schedule": "^4.1.0",
    "@prisma/client": "^5.10.2",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jest-ts-auto-mock": "^2.1.0",
    "nestjs-prisma": "^0.23.0",
    "prisma": "^5.10.2",
    "prisma-dbml-generator": "^0.12.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "semver": "^7.6.0",
    "ts-auto-mock": "^3.7.2",
    "ts-patch": "^3.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vegardit/prisma-generator-nestjs-dto": "^1.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma-docs-generator": "^0.8.0",
    "prisma-generator-fake-data": "^0.14.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "(.spec|.e2e-spec).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "setupFiles": [
      "<rootDir>auto-mock-config.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
